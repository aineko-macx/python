


def check_fermat(a, b, c, n):

    if n <= 2:
        print("n <=2")
    elif a**n + b**n == c**n:
        print("Holy smokes, Fermat was wrong!")
    else:
        print("No, that doesnt work.")

    return

a = int(input("a= \n"))
b = int(input("b= \n"))
c = int(input("c= \n"))
n = int(input("n= \n"))


check_fermat(a, b, c, n)
